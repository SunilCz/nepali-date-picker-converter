{
  "name": "nepali-date-picker-converter",
  "version": "0.1.29",
  "description": "A Nepali date picker and converter library for converting between Nepali (Bikram Sambat) and English (Gregorian) calendars. Works with React, Angular, PHP, Laravel, and vanilla JavaScript.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "unpkg": "dist/bundle.umd.js",
  "browser": "dist/bundle.umd.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/src/index.d.ts"
    },
    "./dist/bundle.react.umd.css": "./dist/bundle.react.umd.css"
  },
  "files": [
    "dist",
    "php",
    "angular",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && npm run build:bundle",
    "build:bundle": "rollup -c",
    "dev": "nodemon --watch src --ext ts --exec ts-node src/index.ts",
    "dev:browser": "vite",
    "preview": "vite preview",
    "prepublishOnly": "npm run build",
    "test": "npm run test:core",
    "test:core": "ts-node test/core.test.ts",
    "test:simple": "node test/simple.test.js",
    "test:browser": "npm run build && npm run dev:browser"
  },
  "keywords": [
    "nepali",
    "date",
    "picker",
    "converter",
    "bikram-sambat",
    "bs",
    "ad",
    "gregorian",
    "calendar",
    "nepal",
    "react",
    "angular",
    "php",
    "laravel",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
